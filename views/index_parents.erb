<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Id</th>
      <th>sfid</th>
      <th>姓</th>
      <th>名</th>
      <th>詳細</th>
    </tr>

    <% @parents.each do |parent| %>
      <tr>
        <td><%= parent.id %></td>
        <td><%= parent.sfid %></td>
        <td><%= parent.sei__c %></td>
        <td><%= parent.mei__c %></td>
        <%= link_to '詳細', parent_detail_path(:id => parent.id) %>
      </tr>
    <% end %>
  </table>

</div>
